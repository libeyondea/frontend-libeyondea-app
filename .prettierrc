{
	"endOfLine": "lf",
	"useTabs": true,
	"printWidth": 160,
	"tabWidth": 4,
	"trailingComma": "none",
	"singleQuote": true,
	"semi": true,
	"importOrder": ["^src|\\.\\/(.*)$"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"pluginSearchDirs": false
}
